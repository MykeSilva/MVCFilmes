@{
    <!--(1-61)-->
    ViewData["Title"] = "Bem vindos";
}

<h1>Bem vindo</h1>


    <!--(2-62)-->
    Olá:@ViewData["Nome"]   
<!--Estou validando oque está sendo digitado (ARQ FilmesController.cs)-->
@if (ViewData["Numerox"] != null)
{
    <ul>
    @for (int i = 0; i <= Convert.ToInt32(ViewData["Numerox"]); i++)
    {
        <span> @i</span>
        <br />
        <li>@i</li>
    }
    </ul>
}